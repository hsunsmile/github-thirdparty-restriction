  "name": "github-thirdparty-restriction",
  "version": "1.0.0",
  "description": "A simple app to ease the process of restricting third party application",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "npm run web-server & npm run worker & npm run webpack",
    "servers": "npm run db-server & npm run web-server",
    "web-server": "bundle exec thin start -p 9393",
    "db-server": "redis-server",
    "worker": "bundle exec sidekiq -r ./src/workers/github/command.rb",
    "compile": "webpack -p --config webpack.config.babel.js",
    "webpack": "webpack --watch --config webpack.config.babel.js"
  },
